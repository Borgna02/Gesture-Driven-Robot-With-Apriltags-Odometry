[
    {
        "type" : "Container",
        "title" : "Processes stat",
        "id" : "ROBOT_PROCESSES_CHART",
        "html" : 
        {
            "template" : "chartjs/chart-container.html",
            "defaultsForVariables" : 
            {
                "ATTRIBUTES" : "style='min-width:100%; min-height:30vh'"
            }
        },
        "init" :
        {
            "template" : "chartjs/init-line-livechart.js",
            "defaultsForVariables" :
            {
                "SAMPLES_LIMIT" : "150",
                "UDM" : "NÂ°"
            }
        },
        "tick" :
        {
            "interval" : 0.5,
            "template" : "chartjs/tick-line-livechart.js"
        },
        "widgets" :
        [
            {
                "type" : "Widget",
                "mode" : "Value",
                "dbName" : "Main",
                "key" : "processesRunning",
                "init" :
                {
                    "template" : "chartjs/init-line-livechart-val.js",
                    "defaultsForVariables" :
                    {
                        "LABEL" : "Running",
                        "COLOR" : "black"
                    }
                },
                "run" : {"template" : "chartjs/run-line-livechart-val.js"}
            },
            {
                "type" : "Widget",
                "mode" : "Value",
                "dbName" : "Main",
                "key" : "processesSleeping",
                "init" :
                {
                    "template" : "chartjs/init-line-livechart-val.js",
                    "defaultsForVariables" :
                    {
                        "LABEL" : "Sleeping",
                        "COLOR" : "magenta"
                    }
                },
                "run" : {"template" : "chartjs/run-line-livechart-val.js"}
            },
            {
                "type" : "Widget",
                "mode" : "Value",
                "dbName" : "Main",
                "key" : "processesStopped",
                "init" :
                {
                    "template" : "chartjs/init-line-livechart-val.js",
                    "defaultsForVariables" :
                    {
                        "LABEL" : "Stopped",
                        "COLOR" : "orange"
                    }
                },
                "run" : {"template" : "chartjs/run-line-livechart-val.js"}
            },
            {
                "type" : "Widget",
                "mode" : "Value",
                "dbName" : "Main",
                "key" : "processesZombie",
                "init" :
                {
                    "template" : "chartjs/init-line-livechart-val.js",
                    "defaultsForVariables" :
                    {
                        "LABEL" : "Zombie",
                        "COLOR" : ""
                    }
                },
                "run" : {"template" : "chartjs/run-line-livechart-val.js"}
            }
        ]
    }
]
