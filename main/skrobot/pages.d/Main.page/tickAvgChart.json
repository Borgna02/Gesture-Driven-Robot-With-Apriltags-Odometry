[
    {
        "type" : "Container",
        "title" : "Tick times average",
        "id" : "ROBOT_TICKAVG_CHART",
        "html" :
        {
            "template" : "chartjs/chart-container.html",
            "defaultsForVariables":
            {
                "ATTRIBUTES" : "class='box' style='min-width: 100%; height:50vh'"
            }
        },
        "init" :
        {
            "template" : "chartjs/init-line-livechart.js",
            "defaultsForVariables" :
            {
                "SAMPLES_LIMIT" : "200",
                "UDM" : "msec"
            }
        },
        "tick" :
        {
            "interval" : 0.4,
            "template" : "chartjs/tick-line-livechart.js"
        },
        "widgets" :
        [
            {
                "type" : "Widget",
                "mode" : "Target",
                "id" : "Main_tickAvg",
                "init" :
                {
                    "template" : "chartjs/init-line-livechart-val.js",
                    "defaultsForVariables" :
                    {
                        "LABEL" : "$$appName$$",
                        "COLOR" : "blue"
                    }
                },
